        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Editors" Height="319" Width="300" FontSize="14" Background="#FFC7C7C7">
	<Border Background="#2e3137" CornerRadius="20" Margin="20">
		<StackPanel Margin="10,0,9.6,-0.4">
			<Label Content="Edytorzy" Foreground="White" FontSize="25" HorizontalAlignment="Center"/>
			<Separator></Separator>
			<ListBox x:Name="listView" Height="159" Background="#545d6a" Foreground="White" FontSize="18" Margin="0,0,-0.4,0"/>
			<Button x:Name="btnSubmit" Click="btnDodaj_Click" Content="Wybierz" Margin="60,8,59.6,10" Background="#545d6a" Foreground="White" FontSize="18"/>

		</StackPanel>
	</Border>
</Window>
